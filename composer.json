{
    "name": "synergitech/laravel-multiconfig",
    "description": "Per-user settings repository system for Laravel",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.43|^9.0|^10.0",
        "illuminate/support": "^8.43|^9.0|^10.0",
        "illuminate/collections": "^8.43|^9.0|^10.0",
        "illuminate/config": "^8.43|^9.0|^10.0",
        "illuminate/cache": "^8.43|^9.0|^10.0",
        "symfony/console": "^5.3|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "autoload": {
        "psr-4": {
            "SynergiTech\\Multiconfig\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SynergiTech\\Multiconfig\\MulticonfigServiceProvider"
            ],
            "aliases": {
                "Setting": "SynergiTech\\Multiconfig\\Facades\\Setting"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
